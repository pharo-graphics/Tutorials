{ #category : #'instance creation' }
MGCardElement class >> inASpace [
	"waiting for Pharo 90 to extend the inspector"
	
	| space card |
	space := BlSpace new.
	card := MGCardElement new. 
	space root addChild: card. 
	space show. 
	{ space . card } inspect
]

{ #category : #accessing }
MGCardElement >> backSideLinesColor [

	^ Color paleBlue
]

{ #category : #'visual properties' }
MGCardElement >> backgroundPaint [
	"Return a BlPaint that should be used as a background (fill)
	of both back and face sides of the card. Color is polymorphic
	with BlPaint and therefore can be used too."
	
	^ Color pink darker
]

{ #category : #accessing }
MGCardElement >> beFlippedBack [

	self removeChildren.
	self addChild: backElement
]

{ #category : #accessing }
MGCardElement >> beFlippedFace [

	self removeChildren.
	self addChild: faceElement

]

{ #category : #accessing }
MGCardElement >> card [
	"Return a card model assigned to this card element"

	^ card
]

{ #category : #accessing }
MGCardElement >> card: aCard [
	"Attach a card model and subscribe to its announcements."
	
	card := aCard.

	faceElement text: (card symbol asString asRopedText
		fontSize: self fontPointSize;
		foreground: self fontColor;
		yourself).

	card isFlipped
		ifTrue: [ self beFlippedFace ]
		ifFalse: [ self beFlippedBack ].

	card announcer
		when: MGCardFlippedAnnouncement
			send: #onFlipped to: self;
		when: MGCardDisappearAnnouncement
			send: #onDisappear to: self
]

{ #category : #'visual properties' }
MGCardElement >> cardExtent [

	^ 80@80
]

{ #category : #accessing }
MGCardElement >> cornerRadius [

	^ 12
]

{ #category : #accessing }
MGCardElement >> fontColor [

	^ Color white
]

{ #category : #accessing }
MGCardElement >> fontFamilyName [

	^ 'Source Sans Pro'
]

{ #category : #accessing }
MGCardElement >> fontPointSize [

	^ 50
]

{ #category : #initialization }
MGCardElement >> initialize [

	super initialize.
	
	backElement := BlElement new
		background: self class cardbackForm;
		size: self cardExtent;
		yourself.

	faceElement := BlTextElement new.

	self size: self cardExtent.
	self layout:
		(BlLinearLayout new
			alignCenter;
			yourself).
	self background: self backgroundPaint.
	self geometry: (BlRoundedRectangleGeometry cornerRadius: self cornerRadius).
	
	self card: (MGCard new symbol: $a).

]

{ #category : #'announcement - handling' }
MGCardElement >> onDisappear [

	| vanish enlarge minimize disappear |
	"when two cards match we want them to enlarge a bit to get player's attention"
	enlarge := BlTransformAnimation scale: 1.15 @ 1.15.
	enlarge
		absolute;
		easing: BlEasing bounceOut;
		duration: 0.5 seconds.

	"vanish animation manipulates opacity to make element transparent"
	vanish := BlOpacityAnimation new
		          opacity: 0;
		          duration: 0.35 seconds.

	"minimization animation changes card's scale to a tiny value"
	minimize := BlTransformAnimation scale: 0.01 @ 0.01.
	minimize
		absolute;
		easing: BlEasing linear;
		duration: 0.35 seconds.

	"disappearing animation makes card transparent and minimizes until it becomes invisible"
	disappear := BlParallelAnimation withAll: { 
			             vanish.
			             minimize }.

	"final animation consists of two sequential animations: enlarge and disappear"
	self addAnimation: (BlSequentialAnimation withAll: { 
				 enlarge.
				 disappear })
]

{ #category : #'announcement - handling' }
MGCardElement >> onFlipped [
	"Update UI after the card is flipped in the model"
	
	card isFlipped
		ifTrue: [ self onFlippedFace ]
		ifFalse: [ self onFlippedBack ]
]

{ #category : #'announcement - handling' }
MGCardElement >> onFlippedBack [

	| animation |
	animation := BlTransformAnimation scale: 1@1.
	animation
		absolute;
		easing: BlEasing bounceOut;
		duration: 0.35 seconds.
	self addAnimation: animation.
	
	self beFlippedBack
]

{ #category : #'announcement - handling' }
MGCardElement >> onFlippedFace [

	| animation |
	animation := BlTransformAnimation scale: 0.85 @ 0.85.	
	animation
		absolute;
		easing: BlQuinticInterpolator default;
		duration: 0.3 seconds.
	self addAnimation: animation.
	
	self beFlippedFace
]
